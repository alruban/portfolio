{
  "name": "portfolio",
  "version": "1.0.0",
  "homepage": "https://www.samuelclarke.dev/",
  "private": false,
  "description": "Portfolio for Sam Clarke's web development projects",
  "main": "index.js",
  "scripts": {
    "web": "node index.js",
    "start": "concurrently --kill-others \"webpack serve --config webpack.dev.js\"",
    "watch": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --stats-children",
    "build-dev": "webpack"
  },
  "engines": {
    "node": "18.7.0"
  },
  "author": "Sam Clarke",
  "license": "ISC",
  "bugs": "https://github.com/alruban/portfolio/issues",
  "repository": {
    "type": "git",

    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "stylelint": "15.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard-scss": "7.0.0",
    "stylelint-order": "6.0.2",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.9.5",
    "url": "^0.11.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.7.3"
  },
  "postcss": {
    "plugins": [
      {
        "postcss-easy-import": {
          "extensions": ".pcss",
          "prefix": "_"
        }
      },
      {
        "postcss-inline-svg": {
          "paths": [
            "src/svgs"
          ]
        }
      },
      {
        "postcss-pxtorem": {
          "propList": [
            "font",
            "font-size",
            "line-height",
            "letter-spacing"
          ]
        }
      },
      "postcss-custom-media",
      "postcss-sassy-mixins",
      "postcss-nested",
      "postcss-simple-vars",
      "autoprefixer"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@rubancorp/react-translate-json": "^1.0.1",
    "@svgr/webpack": "^6.5.0",
    "concurrently": "^7.4.0",
    "minimist": "^1.2.0",
    "postcss-inline-svg": "^5.0.0",
    "prop-types": "^15.8.1",

    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "7.18.6",
    "@babel/runtime": "7.20.13",
    "@types/node": "18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
